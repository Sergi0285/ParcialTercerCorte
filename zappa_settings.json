{
    "dev": {
        "app_function": "crawler.lambda_handler",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "default",
        "project_name": "parcialCrawler",
        "runtime": "python3.9",
        "s3_bucket": "zappa-v63zdflya",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
            {
                "function": "crawler.lambda_handler",  # La función Lambda que quieres ejecutar
                "expression": "cron(0 1 * * ? *)"  # Expresión cron para ejecutarse cada 5 minutos
                
            }
        ]
    }
}